{
    "scripts": {
        "frontend:install": "cd reactnd-contacts-complete && npm install",
        "backend:install": "cd reactnd-contacts-server && npm install",
        "frontend:build": "cd reactnd-contacts-complete && npm run build",
        "backend:build": "cd reactnd-contacts-server && npm run build",
        "install": "npm run frontend:install && npm run backend:install",
        "build": "npm run frontend:build && npm run backend:build",
        "deploy-ui":"cd reactnd-contacts-complete && npm run deploy",
        "deploy-server":"cd reactnd-contacts-server && npm run deploy",
        "deploy":"npm run deploy-ui && npm run deploy-server"
    }
}